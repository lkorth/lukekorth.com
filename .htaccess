# Rewrites
RewriteEngine On
RewriteCond %{HTTP_HOST} ^www\.(.+)$ [NC]
RewriteRule ^(.*)$ http://%1/$1 [R=301,L]

# Cache for 1 hour
<filesMatch "\.(html)$">
    Header set Cache-Control "max-age=3600, public"
</filesMatch>

# Cache for 1 week
<filesMatch "\.(ico|woff|ttf|pdf|jpg|jpeg|png|gif)$">
    Header set Cache-Control "max-age=604800, public"
</filesMatch>